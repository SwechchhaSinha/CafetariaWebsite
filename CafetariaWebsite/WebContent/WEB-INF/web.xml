<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>CafetariaWebsite</display-name>
  <welcome-file-list>
    <welcome-file>HtmlForNavbar.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>FormInputServlet</display-name>
    <servlet-name>FormInputServlet</servlet-name>
    <servlet-class>cafe.controller.FormInput</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FormInputServlet</servlet-name>
    <url-pattern>/InputServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StockGenerateReport</display-name>
    <servlet-name>StockGenerateReport</servlet-name>
    <servlet-class>com.cafe.controller.StockGenerateReport</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StockGenerateReport</servlet-name>
    <url-pattern>/StockGenerateReport</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StockTakeOut</display-name>
    <servlet-name>StockTakeOut</servlet-name>
    <servlet-class>com.cafe.controller.StockTakeOut</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StockTakeOut</servlet-name>
    <url-pattern>/StockTakeOut</url-pattern>
  </servlet-mapping>
</web-app>